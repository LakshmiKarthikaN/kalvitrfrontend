{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "http://kalvitrackweb-env.eba-f54ugkwp.eu-north-1.elasticbeanstalk.com/api/:path*"
      }
    ],
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "X-Forwarded-Host",
            "value": "kalvitrack.vercel.app"
          },
          {
            "key": "X-Forwarded-Proto",
            "value": "https"
          }
        ]
      }
    ]
  }